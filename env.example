# Telegram Channel Bot (for posting signals)
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHANNEL_ID=@your_channel_username

# Admin Bot (for checking stats via Telegram)
# Get token from @BotFather, get your user ID from @userinfobot
ADMIN_BOT_TOKEN=your_admin_bot_token_here
ADMIN_USER_IDS=123456789

# DexScreener API
DEXSCREENER_API=https://api.dexscreener.com/latest/dex

# Polling Intervals (seconds)
POLL_INTERVAL=45
PERFORMANCE_CHECK_INTERVAL_SEC=1800    # 30 minutes
MOMENTUM_CHECK_INTERVAL_SEC=900        # 15 minutes
OUTCOME_CHECK_INTERVAL_SEC=3600        # 1 hour

# Signal Thresholds
MIN_CONVICTION_SCORE=80
WIN_THRESHOLD=100          # +100% = win
LOSS_THRESHOLD=-50         # -50% = loss
EVALUATION_WINDOW_HOURS=24
